{
  "name": "extra-request",
  "version": "8.0.0",
  "description": "Utilities for Request",
  "type": "module",
  "sideEffects": false,
  "keywords": [
    "fetch",
    "Request"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./transformers": "./lib/transformers/index.js",
    "./transformers/*": "./lib/transformers/*.js"
  },
  "engines": {
    "node": ">=16"
  },
  "types": "lib/index.d.ts",
  "repository": "git@github.com:BlackGlory/extra-request.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "prepare": "ts-patch install -s",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --runInBand --config jest.config.js",
    "test:debug": "cross-env NODE_OPTIONS=--experimental-vm-modules node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --runInBand --coverage --config jest.config.js",
    "prepublishOnly": "run-s clean build",
    "clean": "rimraf lib",
    "build": "tsc --project tsconfig.build.json",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s prepare lint test build",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@blackglory/jest-matchers": "^0.4.1",
    "@blackglory/jest-resolver": "^0.1.0",
    "@blackglory/prelude": "^0.1.4",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@types/jest": "^28.1.1",
    "@types/papaparse": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.17.0",
    "extra-fetch": "^3.0.6",
    "husky": "4",
    "jest": "^28.1.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^28.0.4",
    "ts-patch": "^2.0.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3",
    "typescript-transform-paths": "^3.3.1"
  },
  "dependencies": {
    "@blackglory/errors": "^2.2.1",
    "@blackglory/types": "^1.1.0",
    "justypes": "^2.0.0",
    "papaparse": "^5.3.2"
  },
  "peerDependencies": {
    "extra-fetch": "^3.0.0"
  }
}

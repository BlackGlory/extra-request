{
  "name": "extra-request",
  "version": "5.0.1",
  "description": "Utilities for Request",
  "keywords": [
    "fetch",
    "Request"
  ],
  "files": [
    "lib",
    "dist"
  ],
  "main": "lib/es2018/index.js",
  "exports": {
    ".": "./lib/es2018/index.js",
    "./transformers/*": "./lib/es2018/transformers/*",
    "./es2018": "./lib/es2018/index.js",
    "./es2015": "./lib/es2015/index.js",
    "./es2018/transformers/*": "./lib/es2018/transformers/*",
    "./es2015/transformers/*": "./lib/es2015/transformers/*"
  },
  "engines": {
    "node": ">=16"
  },
  "types": "lib/es2018/index.d.ts",
  "repository": "git@github.com:BlackGlory/extra-request.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "jest --runInBand --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:coverage": "jest --runInBand --coverage --config jest.config.js",
    "prepublishOnly": "run-s clean build bundle",
    "clean": "run-p clean:*",
    "clean:build": "rimraf lib",
    "clean:bundle": "rimraf dist",
    "build": "run-p build:*",
    "build:es2015": "run-s build:es2015:*",
    "build:es2015:compile": "tsc --project tsconfig.build.json --module commonjs --target es2015 --outDir lib/es2015",
    "build:es2015:patch": "tscpaths -p tsconfig.build.json -s ./src -o ./lib/es2015",
    "build:es2018": "run-s build:es2018:*",
    "build:es2018:compile": "tsc --project tsconfig.build.json --module commonjs --target es2018 --outDir lib/es2018",
    "build:es2018:patch": "tscpaths -p tsconfig.build.json -s ./src -o ./lib/es2018",
    "bundle": "rollup --config rollup.config.js",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@blackglory/jest-matchers": "^0.3.1",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/jest": "^27.4.0",
    "@types/papaparse": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.8.0",
    "extra-fetch": "^2.0.0",
    "husky": "4",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.67.2",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.3",
    "tscpaths": "^0.0.9",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@blackglory/errors": "^2.2.0",
    "@blackglory/types": "^1.0.1",
    "js-base64": "^3.7.2",
    "justypes": "^2.0.0",
    "papaparse": "^5.3.1"
  },
  "peerDependencies": {
    "extra-fetch": "^2.0.0"
  }
}
